header {
  width: 100%;
  height: 1.70667rem;
  line-height: 1.70667rem;
  text-align: center;
  background-color: #d3bc89;
  color: #fff;
  font-size: 0.48rem;
}

.statusBar {
  height: 0.58667rem;
}

.content {
  left: 0;
  top: 0;
  right: 0;
  width: 100%;
  height: 1.17333rem;
  line-height: 1.17333rem;
  position: relative;
}

.content .leftBtn {
  position: absolute;
  left: 0;
}

.content .return-btn {
  position: absolute;
  left: 0;
  padding: 0.29333rem 0.26667rem 0.29333rem 0.4rem;
}

.content .return-btn:before {
  content: "";
  display: block;
  width: 0.28rem;
  height: 0.58667rem;
  background: url(../image/icon_return.png) no-repeat center;
  background-size: cover;
}

.content .txt-btn {
  left: 0.4rem;
  font-size: 0.37333rem;
}

.rightBtn {
  position: absolute;
  right: 0.4rem;
  width: 0.93333rem;
  height: 1.17333rem;
  line-height: 1.17333rem;
  font-size: 0.37333rem;
}

.search-btn {
  width: 0.53333rem;
  height: 0.53333rem;
  top: 0.29333rem;
  background-image: url(../image/icon_search.png);
  background-size: cover;
}

.back {
  display: block;
  height: 0.56rem;
  background-color: #d3bc89;
  margin-bottom: 1.2rem;
}

.title, .tit {
  width: 9.2rem;
  margin: 0 auto;
}

.por .title {
  height: 1.2rem;
  line-height: 1.2rem;
  background-color: #fff;
  color: #999;
  font-size: 0.42667rem;
  position: absolute;
  top: 0;
  left: 0.4rem;
  border-radius: 0.13333rem;
  box-shadow: 0 0.05333rem 0.13333rem rgba(81, 71, 72, 0.28);
}

.por .title .active {
  color: #d3bc89;
}

.tit {
  font-size: 0.37333rem;
  color: #666;
  margin-bottom: 0.4rem;
}

.list, .list-choice {
  display: none;
}

.list-choice {
  padding-bottom: 1.97333rem;
}

.list li {
  position: relative;
  height: 1.6rem;
}

.list li .btn {
  float: right;
  width: 0.74667rem;
  height: 0.74667rem;
  background: url(../image/del_btn.png) no-repeat center;
  background-size: cover;
  position: absolute;
  right: 1.33333rem;
  top: 0.42667rem;
  display: none;
}

.list li .li {
  height: 1.52rem;
  line-height: 1.52rem;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
  position: absolute;
  z-index: 9;
  background-color: #fff;
}

.list li .li span {
  float: left;
  font-size: 0.42667rem;
  text-align: center;
}

.list li .li span:nth-child(1) {
  width: 3.73333rem;
  position: relative;
  text-align: left;
  text-indent: 0.53333rem;
}

.list li .li span:nth-child(1) i {
  position: absolute;
  height: 0.26667rem;
  line-height: 0.26667rem;
  top: 1.06667rem;
  left: 0;
  font-size: 0.26667rem;
  color: #999;
}

.list li .li span:nth-child(2) {
  color: #4ba54c;
  width: 2.26667rem;
  padding-right: 1.33333rem;
}

.list li .li u {
  height: 0.66667rem;
  line-height: 0.66667rem;
  padding: 0 0.13333rem;
  font-family: 'Arial';
  display: block;
  background-color: #4ba54c;
  border-radius: 0.08rem;
  color: #fff;
  font-size: 0.37333rem;
  margin: 0.4rem 0.4rem 0 0;
}

.list .hot-txt .li span:nth-child(2) {
  color: #e80618;
}

.list .hot-txt .li span:nth-child(3) u {
  background-color: #e80618;
}

.list .theme, .list-theme li {
  position: relative;
  text-align: left;
}

.list .theme .li span:nth-child(1) b, .list-theme li .li span:nth-child(1) b {
  font-size: 0.24rem;
  color: #fff;
  background-color: #e80618;
  height: 0.34667rem;
  line-height: 0.34667rem;
  text-align: center;
  border-radius: 0.08rem;
  top: 0.6rem;
  text-indent: 0;
  padding: 0 0.05333rem;
  margin-left: 0.26667rem;
  position: absolute;
}

.list .theme .li span:nth-child(2), .list-theme li .li span:nth-child(2) {
  color: #dedede;
}

.list .theme, .list-theme li {
  height: 1.2rem;
}

.list .theme .li, .list-theme li .li {
  height: 1.13333rem;
  line-height: 1.13333rem;
}

.list .theme .li span:nth-child(1) b, .list-theme li .li span:nth-child(1) b {
  top: 0.37333rem;
}

.list .theme .li u, .list-theme li .li u {
  margin-top: 0.26667rem;
}

.list .theme .btn, .list-theme li .btn {
  top: 0.24rem;
}

.swipeleft {
  transform: translateX(-30%);
  -webkit-transform: translateX(-30%);
  box-shadow: 0.02667rem 0.02667rem 0.08rem rgba(81, 71, 72, 0.28);
}

.por .chio-box, .chio-min {
  text-align: center;
}

.por .chio-box h2, .chio-min h2 {
  font-size: 0.42667rem;
  color: #333;
  height: 0.45333rem;
  line-height: 0.45333rem;
  padding: 0.53333rem 0 0.24rem 0;
}

.por .chio-box h3, .chio-min h3 {
  font-size: 0.32rem;
  color: #999;
  height: 0.4rem;
  line-height: 0.4rem;
}

.por .chio-box {
  height: 3.73333rem;
}

.por .chio-box a {
  margin-top: 0.26667rem;
}

.chio-min {
  padding-top: 3.46667rem;
}

.chio-min .btn-change {
  width: 2.66667rem;
  height: 0.8rem;
  line-height: 0.8rem;
  font-size: 0.32rem;
  border: 0.02667rem solid #dbdbdb;
  border-radius: 0.4rem;
  margin-top: 0.53333rem;
  color: #999;
  position: relative;
  text-indent: 0.53333rem;
}

.chio-min .btn-change:before {
  content: "";
  width: 0.53333rem;
  height: 0.53333rem;
  position: absolute;
  left: 0.26667rem;
  top: 0.13333rem;
  background: url(../image/btn_change.png) no-repeat center;
  background-size: 0.53333rem;
}

.chio-min p {
  font-size: 0.37333rem;
  color: #666;
  padding-top: 0.8rem;
}

.chio-min .shares-txt span {
  width: 2.26667rem;
  padding: 0 !important;
  position: relative;
  text-indent: 0.13333rem;
}

.chio-min .shares-txt span:before {
  content: "";
  width: 0.37333rem;
  height: 0.37333rem;
  position: absolute;
  left: 0;
  top: 0.05333rem;
  background: url(../image/smIcon.png) no-repeat center;
  background-size: 0.37333rem;
}

.chio-min .signout {
  background-color: #67a6e6;
  margin-top: 1.33333rem;
  box-shadow: 0 0.13333rem 0.26667rem rgba(0, 0, 0, 0.3);
}

.back-news {
  height: 0.96rem;
  margin-bottom: 3.04rem;
}

.news-txt {
  width: 7.86667rem;
  height: 2.4rem;
  border-radius: 0.13333rem;
  position: absolute;
  top: 0;
  left: 0.4rem;
  background: url(../image/news_bg_leon.jpg) no-repeat center;
  background-size: cover;
  color: #fff;
  padding: 0.53333rem 0.66667rem;
}

.news-txt h2 {
  font-size: 0.42667rem;
}

.news-txt p {
  font-size: 0.32rem;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin: 0.26667rem 0;
}

.news-txt .txt {
  width: 100%;
  display: -webkit-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
}

.news-txt .txt span {
  display: block;
  font-size: 0.29333rem;
  font-family: 'Arial';
}

.new-box {
  width: 9.2rem;
  margin: 0 0.4rem 0.4rem 0.4rem;
  padding: 0.26667rem;
  height: 2.66667rem;
  box-shadow: 0 0.02667rem 0.08rem rgba(81, 71, 72, 0.28);
}

.new-box .pic {
  width: 2.13333rem;
  margin-right: 0.4rem;
}

.new-box .pic img {
  height: 2.13333rem;
  border-radius: 0.13333rem;
}

.new-box .min {
  -webkit-box-flex: 1;
}

.new-box .min h3 {
  font-size: 0.37333rem;
}

.new-box .min p {
  font-size: 0.32rem;
  color: #999;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin: 0.21333rem 0;
}

.new-box .min .txt {
  font-family: 'Arial';
  color: #666;
  width: 100%;
  display: -webkit-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
}

.new-box .min .txt span {
  display: block;
  font-size: 0.29333rem;
}

.new-tit {
  font-size: 0.42667rem;
  padding: 0 0.4rem 0.4rem 0.4rem;
}

.share-btn {
  width: 0.53333rem;
  height: 0.53333rem;
  background: url(../image/share_icon_leon.png) no-repeat center;
  background-size: 0.53333rem;
  margin: 0.30667rem 0;
}

.txt-box {
  font-size: 0.37333rem;
}

.txt-box h2 {
  padding: 0.53333rem 0.4rem;
  font-size: 0.48rem;
}

.txt-box .webkit {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  padding: 0 0.4rem;
}

.txt-box .webkit b {
  display: block;
  font-family: 'Arial';
  font-size: 0.32rem;
}

.txt-box img {
  width: 100%;
  margin: 0.4rem auto;
}

.txt-box h3 {
  font-size: 0.42667rem;
  padding: 0.53333rem 0.4rem;
}

.txt-box p {
  padding: 0 0.4rem;
  color: #666;
  text-align: justify;
}

.txt-box p a {
  color: #e80618;
}

.txt-header {
  z-index: 999;
}

.invite-layer {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99;
  display: none;
}

.invite {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 7.73333rem;
  background-color: #fff;
  padding: 0 0.93333rem;
}

.invite h2, .invite p {
  text-align: center;
  font-size: 0.42667rem;
  color: #999;
  padding-top: 0.66667rem;
}

.invite p {
  padding: 0 0 0.66667rem 0;
}

.invite-btn {
  width: 100%;
  display: -webkit-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
}

.invite-btn li {
  color: #666;
  font-size: 0.37333rem;
  text-align: center;
}

.invite-btn li i {
  width: 1.42667rem;
  height: 1.42667rem;
  background: url("../image/invite_leon_pic.png") no-repeat;
  background-size: cover;
  display: block;
  margin: 0 auto 0.4rem;
}

.invite-btn li .invite-icon-wx {
  background-position: 0 0;
}

.invite-btn li .invite-icon-pyq {
  background-position: 0 50%;
}

.invite-btn li .invite-icon-qq {
  background-position: 0 100%;
}

.invite-close {
  width: 100%;
  height: 1.68rem;
  line-height: 1.68rem;
  font-size: 0.4rem;
  color: #666;
  text-align: center;
  border-top: 1px solid #eee;
  margin-top: 1.06667rem;
}
